package multi_client_2;

import java.util.Iterator;

public class ThreadTest extends Thread{

	private long delay;
	
	public ThreadTest(long delay)
	{
		this.delay = delay;
	}
	
	@Override
	public void run()
	{
		for (int i = 0; i < 10; i++) 
		{
			System.out.println(getName()+" "+i);
			
		}
	}
	
	public static void main(String[] args) {
		
	}

}
