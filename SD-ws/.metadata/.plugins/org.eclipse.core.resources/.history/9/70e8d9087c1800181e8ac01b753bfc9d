import java.rmi.Naming;

public class ParrotClient {
	public ParrotClient() 
	{
		try 
		{
			Parrot p = (Parrot) Naming.lookup("rmi://localhost/Guy");
			
			p.repeat("Hello Guy !");
			System.out.println(p.watTime());
		} 
		catch (Exception e) 
		{
			e.printStackTrace();
		}
	}
	
	public static void main(String[] args) {
		new ParrotClient();
	}
}
