public class ClientGUI extends JFrame{
	
	
	
	public ClientGUI() {
		JPanel mainPanel = new JPanel(new BorderLayout());
		add(mainPanel);
		
		
	}
	
	public static void main(String[] args) {
		new ClientGUI();
	}
}
