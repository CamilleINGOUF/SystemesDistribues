package common;

import java.util.ArrayList;

public class Message 
{
	String from;
	ArrayList<String> to;
	
	String message;
	
	public Message(String from, ArrayList<String> to, String message) 
	{
		this.from = from;
		this.to = to;
		
		this.message = message;
	}
	
	public boolean isForAll()
	{
		return to == null;
	}
}
